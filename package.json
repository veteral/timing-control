{
  "name": "time-control",
  "version": "1.0.0",
  "description": "react, express, node",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veteral/timing-control.git"
  },
  "keywords": [
    "react",
    "node",
    "express"
  ],
  "author": "veteral",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veteral/timing-control/issues"
  },
  "homepage": "https://github.com/veteral/timing-control#readme",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "react-datepicker": "^3.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
